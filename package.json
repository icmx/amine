{
  "name": "amine",
  "version": "0.0.3",
  "description": "CSS library",
  "main": "source/amine.scss",
  "scripts": {
    "sass-compile": "npm run sass-compile-expanded && npm run sass-compile-compressed",
    "sass-compile-expanded": "sass --style expanded --no-source-map source/amine.scss output/amine.css",
    "sass-compile-compressed": "sass --style compressed --no-source-map source/amine.scss output/amine.min.css",
    "sass-watch": "sass --style expanded --no-source-map --watch source/amine.scss output/amine.watch.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icmx/amine.git"
  },
  "keywords": [
    "css",
    "css-framework"
  ],
  "author": "icmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icmx/amine/issues"
  },
  "homepage": "https://github.com/icmx/amine",
  "dependencies": {
    "sass": "^1.26.10"
  }
}
