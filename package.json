{
  "name": "amine",
  "version": "0.0.8",
  "description": "CSS library",
  "main": "source/amine.scss",
  "scripts": {
    "compile": "npm run compile:amine && npm run compile:amine.min && npm run compile:docs",
    "compile:amine": "sass --style expanded --no-source-map scss/amine.scss css/amine.css",
    "compile:amine.min": "sass --style compressed --no-source-map scss/amine.scss css/amine.min.css",
    "compile:docs": "sass --style expanded --no-source-map scss/amine.scss docs/amine.css",
    "watch": "sass --style expanded --no-source-map --watch scss/amine.scss docs/amine.watch.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icmx/amine.git"
  },
  "keywords": [
    "css",
    "css-library"
  ],
  "author": "icmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icmx/amine/issues"
  },
  "homepage": "https://github.com/icmx/amine",
  "dependencies": {
    "sass": "^1.26.10"
  }
}
